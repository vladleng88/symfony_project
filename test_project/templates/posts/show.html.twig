{% extends 'base.html.twig' %}
{% block title %}Post requested{% endblock %}
{% block body %}
    <h1> {{ post.title }}</h1>
<p>{{ post.body }}</p>
    <a href="{{ path('post_update', {slug:post.slug}) }}">Редактировать</a>
    <a href="{{ path('post_delete', {slug:post.slug}) }}">Удалить</a>
{% endblock %}